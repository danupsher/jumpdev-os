# JumpDev OS Wofi Config
# Clean, minimal launcher with coder aesthetic

# Size and position
width=400
height=500
location=top-left
xoffset=10
yoffset=44

# Behavior
allow_markup=true
allow_images=true
insensitive=true
prompt=
hide_scroll=false
no_actions=true
matching=fuzzy
